
config wifi-device 'wifi0'
	option type 'qcawifi'
	option channel 'auto'
	option macaddr 'b4:ee:b4:e9:ab:03'
	option hwmode '11ac'
	option country '843'
	option htmode 'HT80'
	option chbandwh '4'
	option mode '5G'
	option txchainmask '15'
	option rxchainmask '15'
	option tpscale '0'
	option burst '1'
	option wlshcenable '0'
	option dcs_enable '0'
	option obss_rx_rssi_th '35'
	option obss_rssi_th '35'
	option acs_bkscanen '1'
	option acs_rssivar '10'
	option thrput_enab '1'
	option PER_enab '1'
	option enable_ol_stats '1'
	option atfstrictsched '1'
	option disabled '0'

