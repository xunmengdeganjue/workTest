
config interface 'loopback'
	option ifname 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config globals 'globals'
	option ula_prefix 'auto'

config interface 'lan'
	option ifname 'eth1'
	option type 'bridge'
	option proto 'static'
	option netmask '255.255.255.0'
	option ip6assign '60'
	option igmp_snooping '0'
	option ipaddr '192.168.2.1'

config interface 'wan'
	option ifname 'eth0'
	option proto 'dhcp'

config interface 'wan6'
	option ifname 'eth0'

config interface 'commwan'
	option ifname 'eth0'
	option mtu '1500'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '1'
	option ports '6 1 2 3 4'

config switch_vlan
	option device 'switch0'
	option vlan '2'
	option ports '0 5'

config interface 'com'
	option type 'bridge'
	option proto 'static'
	option ipaddr '12.0.0.50'
	option netmask '255.255.255.0'

config interface 'commtap'
	option type 'bridge'

config interface 'veth'
	option proto 'veth'
	option ifname 'veth0'
	option network 'com'
	option peer_ifname 'veth1'
	option peer_network 'commtap'
	option enable_vlan '1'
	option vlanid '7'

config interface 'tap'
	option proto 'gretap'
	option network 'commtap'
	option enable_vlan '1'
	option ipaddr '11.0.0.1'
	option peeraddr '11.0.0.6'
	option ka_pri_addr '11.0.0.6'
	option ka_sec_addr '11.0.0.110'
	option ka_intrvl '3'
	option ka_thrld '3'
	option ka_pri_chk_intrvl '3'

config interface 'tagv'
	option ifname 'eth0.5'
	option proto 'static'
	option ipaddr '11.0.0.1'
	option netmask '255.255.255.0'

config switch_vlan 'specvlan'
	option device 'switch0'
	option vlan '5'
	option ports '0t 5t'

